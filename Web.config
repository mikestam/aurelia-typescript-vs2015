<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.6"/>
    <httpRuntime targetFramework="4.6"/>
  </system.web>

  <system.webServer>
    <defaultDocument>
      <files>
        <clear/>
        <add value="index.html" />
      </files>
    </defaultDocument>
    <directoryBrowse enabled="true" />

    <security>
      <requestFiltering>
        <hiddenSegments>
          <clear/>
        </hiddenSegments>
      </requestFiltering>
    </security>
    
    
  </system.webServer>
</configuration>